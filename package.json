{
  "name": "cmc-core",
  "version": "1.0.0",
  "description": "making a mapping app with react, redux, and friends",
  "scripts": {
    "prestart": "npm-run-all --parallel remove-dist lint:scripts",
    "start": "npm run open:src",
    "open:src": "babel-node scripts/srcServer.js",
    "open:dist": "babel-node scripts/distServer.js",
    "lint:scripts": "eslint webpack.config.js scripts",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node scripts/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html",
    "build": "babel-node scripts/build.js > dist/build-report.txt && npm run postbuild",
    "build:open": "npm run build && npm run open:dist",
    "test": "cross-env NODE_ENV=test karma start karma.conf.js --singleRun=true --reporters=progress,html",
    "test:watch": "cross-env NODE_ENV=test karma start karma.conf.js --singleRun=false --reporters=progress,html",
    "test:cover": "cross-env NODE_ENV=test karma start karma.conf.js --singleRun=true",
    "test:cover:watch": "cross-env NODE_ENV=test karma start karma.conf.js --singleRun=false",
    "test:skipWebGLTests": "cross-env NODE_ENV=test SKIP_WEBGL_TESTS=true karma start karma.conf.js --singleRun=true --reporters=progress,html",
    "test:skipWebGLTests:watch": "cross-env NODE_ENV=test SKIP_WEBGL_TESTS=true karma start karma.conf.js --singleRun=false --reporters=progress,html",
    "test:skipWebGLTests:cover": "cross-env NODE_ENV=test SKIP_WEBGL_TESTS=true karma start karma.conf.js --singleRun=true",
    "test:skipWebGLTests:cover:watch": "cross-env NODE_ENV=test SKIP_WEBGL_TESTS=true karma start karma.conf.js --singleRun=false",
    "postinstall": "scripts/postinstall.sh",
    "postbuild": "scripts/postbuild.sh",
    "deploy": "scripts/deploy.bash",
    "deploy:docker": "scripts/dockerDeploy.bash"
  },
  "author": "Flynn Platt",
  "license": "MIT",
  "dependencies": {
    "cesium": "1.29.0",
    "connect-history-api-fallback": "1.2.0",
    "d3": "3.5.17",
    "es6-promise": "3.1.2",
    "flexboxgrid": "6.3.0",
    "isomorphic-fetch": "2.2.1",
    "karma-coverage": "1.1.1",
    "karma-phantomjs-launcher": "1.0.2",
    "normalize.css": "4.2.0",
    "object-assign": "4.0.1",
    "phantomjs-prebuilt": "2.1.14",
    "react": "15.3.1",
    "react-addons-css-transition-group": "15.3.0",
    "react-addons-update": "15.3.0",
    "react-dom": "15.3.1",
    "react-ga": "2.1.2",
    "react-key-handler": "0.2.0",
    "react-redux": "4.4.5",
    "react-tap-event-plugin": "1.0.0",
    "react-toolbox": "1.2.1",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "script-loader": "0.7.0",
    "url-loader": "0.5.7",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "6.6.1",
    "babel-cli": "6.11.4",
    "babel-core": "6.11.4",
    "babel-eslint": "6.0.2",
