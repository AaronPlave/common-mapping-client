{
  "name": "react-map",
  "version": "0.0.1",
  "description": "making a mapping app with react, redux, and friends",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist lint:tools",
    "start": "npm-run-all --parallel test:watch open:src",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint:tools": "eslint webpack.config.js tools",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "karma start karma.conf.js --singleRun=true",
    "test:watch": "karma start karma.conf.js --singleRun=false",
    "postinstall": "bash ./postinstall.sh"
  },
  "author": "Flynn Platt",
  "dependencies": {
    "cesium": "1.21.0",
    "connect-history-api-fallback": "1.2.0",
    "d3": "3.5.17",
    "es6-promise": "3.1.2",
    "flexboxgrid": "6.3.0",
    "isomorphic-fetch": "2.2.1",
    "normalize.css": "4.1.1",
    "object-assign": "4.0.1",
    "react": "15.0.1",
    "react-addons-css-transition-group": "15.0.2",
    "react-custom-scrollbars": "4.0.0-beta.2",
    "react-dom": "15.0.1",
    "react-key-handler": "0.2.0",
    "react-redux": "4.4.0",
    "react-tap-event-plugin": "1.0.0",
    "react-toolbox": "0.16.2",
    "redux": "3.4.0",
    "redux-thunk": "2.1.0",
    "script-loader": "0.7.0",
    "url-loader": "0.5.7"
  },
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-core": "6.7.6",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.5.0",
    "browser-sync": "2.11.2",
    "chai": "3.5.0",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "2.7.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-import": "1.4.0",
    "eslint-plugin-react": "4.3.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "immutable": "3.8.1",
    "karma": "0.13.22",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-cli": "1.0.0",
    "karma-mocha": "1.0.1",
    "karma-phantomjs-launcher": "1.0.0",
    "karma-requirejs": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "mocha": "2.4.5",
    "moment": "2.13.0",
    "node-sass": "3.4.2",
    "npm-run-all": "1.7.0",
    "openlayers": "3.15.1",
    "phantomjs-prebuilt": "2.1.7",
    "react-addons-test-utils": "15.0.1",
    "react-dom": "15.0.1",
    "react-key-handler": "0.2.0",
    "react-tooltip": "3.0.4",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "sinon": "1.17.3",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "toolbox-loader": "0.0.3",
    "webpack": "1.12.15",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://podaac-git.jpl.nasa.gov:8443/fplatt/react-map.git"
  }
}
